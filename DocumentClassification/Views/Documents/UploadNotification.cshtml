@model IEnumerable<DocumentClassification.Models.Document>
@{
    ViewData["Title"] = "UploadNotification";
}

<h1>Uploaded Documents</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Document
            </th>
            <th>
                Predicted Category
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Name
                </td>

                <td>
                    @item.Category.Name
                </td>
            </tr>
        }
    </tbody>
</table>
